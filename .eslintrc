{
  "parserOptions": {
    "ecmaVersion": 5
  },
  "env": {
    "browser": true
  },
  "rules": {
    // best practice
    "guard-for-in": 2,
    "no-extend-native": 2,
    "no-console": 1,
    "no-extra-bind": 2,
    "no-new-wrappers": 2,
    "no-throw-literal": 2,
    "no-fallthrough": 2,
    "no-redeclare": 2,
    "no-unused-labels": 2,
    "no-unused-vars": [
      2,
      {
        "args": "none"
      }
    ],
    "no-undef": 2,
    // stylistic
    "brace-style": 2,
    "comma-spacing": 2,
    "eol-last": 2,
    "key-spacing": 2,
    "array-bracket-spacing": [
      2,
      "never"
    ],
    "comma-style": 2,
    "computed-property-spacing": 2,
    "func-call-spacing": 2,
    "keyword-spacing": 0,
    "linebreak-style": 2,
    "no-array-constructor": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": [
      2,
      {
        "max": 2
      }
    ],
    "no-new-object": 2,
    "no-trailing-spaces": 0,
    "object-curly-spacing": 0
  },
  "globals": {
    "replaceDataLayer": true,
    "jQuery": true,
    "Backbone": true,
    "ImageCompressor": true,
    "_": true,
    "$": true,
    "moment": true,
    "rrRenderedFlag": true,
    "retailrocket": true,
    "dataLayer": true,
    "als": true,
    "Storage": true,
    "confirm": true,
    "VK": true,
    "jasmine": true,
    "oko": true,
    "afterEach": false,
    "beforeEach": false,
    "context": false,
    "describe": false,
    "expect": false,
    "it": false,
    "mostRecentAjaxRequest": false,
    "runs": false,
    "spyOn": false,
    "spyOnEvent": false,
    "waitsFor": false,
    "xdescribe": false,
    "readFixtures": false,
    "sinon": false,
    "Raven": true,
    "LiveTexApi": true,
    "videojs": true,
    "audiojs": true,
    "SmartBanner": true,
    "DCLoans": true,
    "getJSONFixture": true,
    "loadJSONFixtures": true,
    "loadFixtures": true
  }
}
